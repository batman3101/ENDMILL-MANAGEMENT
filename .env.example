# Supabase 연결 정보
# Supabase 대시보드에서 확인: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Supabase MCP 액세스 토큰 (Claude Code용)
# Supabase 대시보드에서 발급: https://supabase.com/dashboard/account/tokens
SUPABASE_ACCESS_TOKEN=your_supabase_access_token

# 개발 환경 설정
NODE_ENV=development

# 임시 인증 시스템 (개발용 - 프로덕션에서는 제거)
TEMP_AUTH_ENABLED=true
TEMP_ADMIN_EMAIL=your_admin_email
TEMP_ADMIN_PASSWORD=your_admin_password
TEMP_ADMIN_NAME=your_admin_name
TEMP_ADMIN_DEPARTMENT=your_department
TEMP_ADMIN_POSITION=your_position
TEMP_ADMIN_ROLE=system_admin
